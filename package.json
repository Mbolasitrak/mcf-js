{
  "name": "mcf",
  "version": "1.0.5",
  "description": "Modular Crypt Format",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/madarche/mcf-js.git"
  },
  "keywords": [
    "modular",
    "crypt",
    "format",
    "crypto",
    "database",
    "db"
  ],
  "author": "Marc-Aur√®le DARCHE <ma.darche@cynode.org> (https://github.com/madarche)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madarche/mcf-js/issues"
  },
  "homepage": "https://github.com/madarche/mcf-js",
  "dependencies": {
    "create-error": "0.3.1"
  },
  "devDependencies": {
    "blanket": "1.1.7",
    "coveralls": "2.11.2",
    "jshint": "2.8.0",
    "mocha": "2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "must": "0.12.0"
  },
  "jshintConfig": {
    "unused": "vars",
    "undef": true,
    "node": true,
    "mocha": true
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js"
      ],
      "data-cover-never": [
        "node_modules"
      ]
    }
  },
  "scripts": {
    "test": "jshint *.json *.js test/*.js && mocha --check-leaks -R spec",
    "test-coverage": "jshint *.json *.js test/*.js && mocha --require blanket -R html-cov > test/coverage.html",
    "test-travis": "jshint *.json *.js test/*.js && mocha --check-leaks --require blanket -R mocha-lcov-reporter | coveralls",
    "clean": "rm -rf test/coverage.html"
  }
}
